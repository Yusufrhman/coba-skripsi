  "E": [
    {
      "id": "landing_home->blog_detail",
      "from": "landing_home",
      "to": "blog_detail",
      "conditions": "userTapsNewsArticleOnHomePage",
      "actions": "loadBlogDetail"
    },
    {
      "id": "blog_list->blog_detail",
      "from": "blog_list",
      "to": "blog_detail",
      "conditions": "userTapsNewsArticleOnBlogListPage",
      "actions": "loadBlogDetail"
    },
    {
      "id": "landing_home->blog_list",
      "from": "landing_home",
      "to": "blog_list",
      "conditions": "userTapsNewsLink",
      "actions": "loadBlogList"
    },
    {
      "id": "blog_detail->blog_list",
      "from": "blog_detail",
      "to": "blog_list",
      "conditions": "userTapsBackButton",
      "actions": "loadBlogList"
    },
    {
      "id": "landing_home->event_list",
      "from": "landing_home",
      "to": "event_list",
      "conditions": "userTapsMenuAndSelectsEvent",
      "actions": "loadEventList"
    },
    {
      "id": "blog_list->landing_home",
      "from": "blog_list",
      "to": "landing_home",
      "conditions": "userTapsHomeButton",
      "actions": "loadHomePage"
    },
    {
      "id": "blog_detail->landing_home",
      "from": "blog_detail",
      "to": "landing_home",
      "conditions": "userTapsHomeButton",
      "actions": "loadHomePage"
    },
    {
      "id": "event_list->landing_home",
      "from": "event_list",
      "to": "landing_home",
      "conditions": "userTapsHomeButton",
      "actions": "loadHomePage"
    }
  ]
}