  "E": [
    {
      "id": "root->blog_list_header",
      "from": "root",
      "to": "blog_list",
      "conditions": "userTapsNewsHeaderLink",
      "actions": "loadBlogListPage"
    },
    {
      "id": "root->event_list_header",
      "from": "root",
      "to": "event_list",
      "conditions": "userTapsCommunityHeaderLink",
      "actions": "loadEventListPage"
    },
    {
      "id": "root->blog_list_from_preview_button",
      "from": "root",
      "to": "blog_list",
      "conditions": "userTapsSeeAllNewsButton",
      "actions": "loadBlogListPage"
    },
    {
      "id": "root->blog_detail_from_preview",
      "from": "root",
      "to": "blog_detail",
      "conditions": "userTapsLatestNewsArticlePreview",
      "actions": "loadBlogDetailPageWithArticleData"
    },
    {
      "id": "blog_list->root_header",
      "from": "blog_list",
      "to": "root",
      "conditions": "userTapsHomeHeaderLink",
      "actions": "loadHomePage"
    },
    {
      "id": "blog_list->blog_list_load_more",
      "from": "blog_list",
      "to": "blog_list",
      "conditions": "userTapsLoadMoreButton",
      "actions": "loadMoreArticles"
    },
    {
      "id": "blog_list->blog_detail_from_card",
      "from": "blog_list",
      "to": "blog_detail",
      "conditions": "userTapsArticleCard",
      "actions": "loadBlogDetailPageWithArticleData"
    },
    {
      "id": "blog_list->event_list_header",
      "from": "blog_list",
      "to": "event_list",
      "conditions": "userTapsCommunityHeaderLink",
      "actions": "loadEventListPage"
    },
    {
      "id": "blog_detail->root_header",
      "from": "blog_detail",
      "to": "root",
      "conditions": "userTapsHomeHeaderLink",
      "actions": "loadHomePage"
    },
    {
      "id": "blog_detail->blog_list_header",
      "from": "blog_detail",
      "to": "blog_list",
      "conditions": "userTapsNewsHeaderLink",
      "actions": "loadBlogListPage"
    },
    {
      "id": "blog_detail->blog_detail_other_news",
      "from": "blog_detail",
      "to": "blog_detail",
      "conditions": "userTapsOtherNewsArticle",
      "actions": "loadBlogDetailPageWithNewArticleData"
    },
    {
      "id": "blog_detail->event_list_header",
      "from": "blog_detail",
      "to": "event_list",
      "conditions": "userTapsCommunityHeaderLink",
      "actions": "loadEventListPage"
    },
    {
      "id": "event_list->root_header",
      "from": "event_list",
      "to": "root",
      "conditions": "userTapsHomeHeaderLink",
      "actions": "loadHomePage"
    },
    {
      "id": "event_list->blog_list_header",
      "from": "event_list",
      "to": "blog_list",
      "conditions": "userTapsNewsHeaderLink",
      "actions": "loadBlogListPage"
    },
    {
      "id": "event_list->event_list_load_more",
      "from": "event_list",
      "to": "event_list",
      "conditions": "userTapsLoadMoreButton",
      "actions": "loadMoreEvents"
